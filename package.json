    "base-option": "^0.8.2",
    "base-pkg": "^0.2.3",
    "base-plugins": "^0.4.13",
    "base-task": "^0.6.1",
    "debug": "^2.2.0",
    "define-property": "^0.2.5",
    "extend-shallow": "^2.0.1",
    "global-modules": "^0.2.2",
    "is-valid-app": "^0.1.0",
